<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.studentsnick.LoginController">
   <children>
      <Label layoutX="96.0" layoutY="109.0" text="username:">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Label>
      <Label layoutX="96.0" layoutY="164.0" text="password:">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Label>
      <TextField fx:id="name" layoutX="209.0" layoutY="112.0" prefHeight="25.0" prefWidth="212.0">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <PasswordField fx:id="password" layoutX="209.0" layoutY="168.0" prefHeight="25.0" prefWidth="212.0">
         <font>
            <Font size="14.0" />
         </font></PasswordField>
      <Button fx:id="loginButton" layoutX="209.0" layoutY="215.0" mnemonicParsing="false" onAction="#register" prefHeight="30.0" prefWidth="95.0" text="Enter">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutX="326.0" layoutY="215.0" mnemonicParsing="false" onAction="#cancel" prefHeight="30.0" prefWidth="95.0" text="Cancel">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
